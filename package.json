{
  "name": "octoo-static",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit:ci",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
